{
    "extends": [
        "config:recommended",
        ":dependencyDashboard"
    ],
    "customManagers": [
        {
            "customType": "regex",
            "managerFilePatterns": [
                "(^|/)execution-environment\\.yml$"
            ],
            "matchStrings": [
                "name:\\s+(?<depName>[a-zA-Z0-9.-]+(?:/[a-zA-Z0-9.-]+)+):(?<currentValue>[\\w.-]+)"
            ],
            "datasourceTemplate": "docker"
        },
        {
            "customType": "regex",
            "managerFilePatterns": [
                "(^|/)execution-environment\\.yml$"
            ],
            "matchStrings": [
                "package_pip:\\s+(?<depName>[a-zA-Z0-9_-]+)==(?<currentValue>[a-zA-Z0-9.-]+)"
            ],
            "datasourceTemplate": "pypi"
        }
    ]
}
